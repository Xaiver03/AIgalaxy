{
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install && npx prisma generate",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "devCommand": "npm run dev",
  "env": {
    "FORCE_CACHE_REFRESH": "v2.1.4"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}